<?xml version="1.0" encoding="UTF-8"?>
<maths xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="maths.xsd">
    <!-- (2 * 3) -->
    <apply>
        <times />
        <cn>2</cn>
        <cn>3</cn>
    </apply>
    <!-- 2 * (2 + 3) -->
    <apply>
        <divide />
        <cn>2</cn>
        <apply>
            <plus />
            <cn>2</cn>
            <cn>3</cn>
        </apply>
    </apply>
    <!-- (2 * 3) + (4 * 5) -->
    <apply>
        <plus />
        <apply>
            <times />
            <cn>2</cn>
            <cn>3</cn>
        </apply>
        <apply>
            <times />
            <cn>4</cn>
            <cn>5</cn>
        </apply>
    </apply>
</maths>